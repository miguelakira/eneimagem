body {
	font-family: Verdana, arial; 
	margin: 0 auto;
	width: 775px;
	background: #fff url('../img/background-novo.jpg') repeat fixed 0 0;
	}
	
#container
{
	margin: 0 auto;
	width: 770px;
	
}

#header
{
	background: #66CDAA;
	filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#66CDAA', endColorstr='#458B74');
	background: -webkit-gradient(linear, left top, left bottom, from(#66CDAA), to(#458B74));
	background: -moz-linear-gradient(top, #66CDAA, #458B74);
	color:#141414;
	padding: 20px;
	
	
}

#header h1 { 
	margin: 0; }

#navigation
{
	float: left;
	width: 770px;
	background: #333;
	filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#333333', endColorstr='#777777');
	background: -moz-linear-gradient(top, #333, #777) repeat-X;
	background: -webkit-gradient(linear, left top, left bottom, from(#333), to(#777)) repeat-X;	
}

#navigation ul
{
	margin: 0;
	padding: 0;
}

#navigation ul li
{
	list-style-type: none;
	display: inline;
}

#navigation li a
{
	display: block;
	float: left;
	padding: 5px 10px;
	color: #fff;
	text-decoration: none;
	border-right: 1px solid #fff;
	
}

#navigation li a:hover { background: #458B74; }

#content-container
{
	margin-top:10px;
	float: left;
	width: 770px;
	background: #fff url('../img/background-risco-cinza-escuro.gif') repeat;
	border: 2px solid #C2BBAC;
	
	
}

#section-navigation
{
	float: left;
	width: 170px;
	padding: 0;
	margin: 15px 5px 10px 10px;
	display: inline;
	background: #fff;
	border: 1px solid #CCCCCC;
	
	
}

#section-navigation ul
{
	margin: 0;	
	padding: 0;
	background: white;
}

#section-navigation ul li
{
	
	background: #EBEAEA;
	margin: 5px;
	padding: 0;
	list-style-type: none;
	border-left:1px solid #ccc;
	border-right:1px solid #ccc;
	border-bottom: 1px solid #ccc;
	
}

#section-navigation ul li a{
	display: block;
	text-decoration: none;
	color: #333;
	font-weight: bold;
	font-size:0.7em;
	padding: 10px 5px;
}

#section-navigation li a:hover { 
	background: #458B74; 
	color: #fff;
}

#content
{
	float: left;
	width: 500px;
	padding: 20px 30px  ;
	margin: 15px 0px 10px 7px;
	background: #fff;
	border: 1px solid #CCCCCC;
}

#content h2 { margin: 0; }

#footer
{
	clear: left;
	background: #333;
	background: -moz-linear-gradient(top, #777, #333) repeat-X;
	background: -webkit-gradient(linear, left top, left bottom, from(#777), to(#333)) repeat-X;
	text-align: right;
	padding:20px 0;
	height: 1%;
	width:100%;
	color: #fff;
	text-align:center;
	border-left: 2px solid #C2BBAC;
	border-right: 2px solid #C2BBAC;
	
		
}


